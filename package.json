{
  "private": true,
  "name": "treehouse-vue",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "lerna bootstrap --hoist",
    "serve:flashcard": "lerna run --stream --scope=@treehouse-vue/flashcard serve",
    "serve:library": "lerna run --stream --scope=@treehouse-vue/library serve",
    "build:flashcard": "lerna run --stream --scope=@treehouse-vue/flashcard build",
    "build:library": "lerna run --stream --scope=@treehouse-vue/library build",
    "lint:flashcard": "lerna run --stream --scope=@treehouse-vue/flashcard lint",
    "lint:library": "lerna run --stream --scope=@treehouse-vue/library lint",
    "unit:flashcard": "lerna run --stream --scope=@treehouse-vue/flashcard test:unit",
    "unit:library": "lerna run --stream --scope=@treehouse-vue/library test:unit",
    "e2e:flashcard": "lerna run --stream --scope=@treehouse-vue/flashcard test:e2e",
    "e2e:library": "lerna run --stream --scope=@treehouse-vue/library test:e2e"
  },
  "dependencies": {
    "shortid": "^2.2.0",
    "vue": "^2.5.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "lerna": "^3.6.0",
    "vue-eslint-parser": "^4.0.0",
    "vue-template-compiler": "^2.5.0"
  }
}
